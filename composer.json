{
    "name": "bytaj/birthay-greetings-kata-php",
    "description": "A simple refactoring exercise that is meant to teach something about dependency inversion and dependency injection",
    "version": "2.0.0",
    "require": {
        "ext-json": "*",
        "php": ">=8.2",
        "symfony/mailer": "^6.2"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.5",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "symfony/process": "^6.2"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BirthdayGreetingsKata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BirthdayGreetingsKata\\": "tests/"
        }
    }
}
